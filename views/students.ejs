<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>data is here</h1>

    <table border="2px">
      <tr>
        <td>name</td>
        <td>age</td>
        <td>email</td>
        <td>Operation</td>
      </tr>
      <% students.forEach(student=>{ %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.age %></td>
        <td><%= student.email %></td>
        <td><a href="ui/delete/<%= student._id %>">Delete</a></td>
        <td><a href="ui/student/<%= student._id %>">Update</a></td>
      </tr>
      <%})%>
    </table>
  </body>
</html>
